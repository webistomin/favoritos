{
  "name": "favoritos",
  "version": "1.0.0",
  "description": "Favoritos is a JavaScript plugin that adds some HTML5 canvas magic to your favicon. With just a wee bit of code, we can make some really cool effects.",
  "reveal": true,
  "keywords": [
    "favicon",
    "favicons",
    "favicon-badge",
    "favicon-manager",
    "favicon-dynamic"
  ],
  "main": "./dist/favoritos.cjs.js",
  "module": "./dist/favoritos.esm.js",
  "browser": {
    "dist/favoritos.js": "./dist/favoritos.iife.js",
    "dist/favoritos.esm.js": "./dist/favoritos.esm.js"
  },
  "unpkg": "./dist/favoritos.iife.js",
  "jsdelivr": "./dist/favoritos.iife.js",
  "typings": "./dist/favoritos.d.ts",
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "run-s prebuild build:all build:dts",
    "build:all": "cross-env NODE_ENV=production rollup --config config/rollup.config.js",
    "build:cjs": "cross-env NODE_ENV=production rollup --config config/rollup.config.js --format cjs",
    "build:dts": "tsc -d --declarationDir dist --declarationMap --emitDeclarationOnly",
    "build:es": "cross-env NODE_ENV=production rollup --config config/rollup.config.js --format es",
    "build:iife": "cross-env NODE_ENV=production rollup --config config/rollup.config.js --format iife",
    "dev": "rollup -c config/rollup.config.js -w",
    "lint": "run-s eslint:write prettier:write",
    "eslint:write": "eslint --ext .js,.tsx,.ts . --fix",
    "prettier:write": "prettier --write \"**/*.+(js|ts|tsx)\"",
    "sizelimit": "size-limit",
    "typedoc": "typedoc --out docs --target es6 --theme minimal --mode file src",
    "prebuild": "rimraf dist",
    "start": "npm run dev",
    "test:unit": "jest --config jest.config.js --coverage",
    "test:unit:watch": "jest --config jest.config.js --coverage --watch",
    "e2e:server": "run-s delete:dist copy:dist serve",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run",
    "delete:dist": "rimraf test/e2e/fixtures/dist",
    "copy:dist": "cp dist/ test/e2e/fixtures -r",
    "serve": "http-server -p 3030 ./test/e2e/fixtures"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webistomin/favoritos.git"
  },
  "author": "webistomin",
  "email": "webistomin@gmail.com",
  "license": "MIT",
  "engines": {
    "node": ">=10"
  },
  "bugs": {
    "url": "https://github.com/webistomin/favoritos/issues"
  },
  "homepage": "https://github.com/webistomin/favoritos#readme",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-angular": "^9.0.1",
    "@cypress/webpack-preprocessor": "^5.4.1",
    "@ls-lint/ls-lint": "^1.8.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-inject": "^4.0.1",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@size-limit/preset-small-lib": "^4.4.5",
    "@size-limit/time": "^4.4.5",
    "@types/cypress-image-snapshot": "^3.1.2",
    "@types/jest": "^26.0.0",
    "@types/jsdom": "^16.2.3",
    "@types/node": "^14.0.12",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^26.0.1",
    "cypress": "^5.0.0",
    "cypress-image-snapshot": "^3.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-fetch-mock": "^3.0.3",
    "lint-staged": "^10.1.3",
    "lodash.capitalize": "^4.2.1",
    "minimist": "^1.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.6.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-banner2": "^1.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.2",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "rollup-plugin-web-worker-loader": "^1.3.0",
    "scriptlint": "^2.1.1",
    "size-limit": "^4.4.5",
    "ts-jest": "^25.3.1",
    "ts-loader": "^8.0.0",
    "tslib": "^2.0.0",
    "typedoc": "^0.19.1",
    "typescript": "^3.8.3"
  },
  "dependencies": {}
}
